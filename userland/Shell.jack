class Shell {
    function void main() {
        var char c, key;
    	var String s;
    	var boolean ok;
        while (true) {
            let s = Keyboard.readLine("$ ");
            do Output.printString(s);
            if (String.compare(s,"vi") = 0) {
                do Vim.main();
            } else {
                do Output.printString("Command not found.");
            }

            do Output.println();
	    }
        return;
    }
}
